Cell[BoxData[{
 RowBox[{
  RowBox[{"twitter", "=", 
   RowBox[{"ServiceConnect", "[", 
    RowBox[{"\"\<Twitter\>\"", ",", "\"\<New\>\""}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"result", "=", 
   RowBox[{"twitter", "[", 
    RowBox[{"\"\<TweetSearch\>\"", ",", 
     RowBox[{"\"\<Query\>\"", "->", "\"\<from:polfosol\>\""}], ",", 
     RowBox[{"MaxItems", "->", "5000"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"data", "=", 
    RowBox[{"DeleteCases", "[", 
     RowBox[{
      RowBox[{"Flatten", "[", 
       RowBox[{"Normal", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"StringSplit", "[", 
           RowBox[{"#", "[", "\"\<Text\>\"", "]"}], "]"}], "&"}], "/@", 
         "result"}], "]"}], "]"}], ",", 
      RowBox[{"x_", "/;", 
       RowBox[{
        RowBox[{
         RowBox[{"StringLength", "[", "x", "]"}], "<", "4"}], "||", 
        RowBox[{"StringContainsQ", "[", 
         RowBox[{"x", ",", "\"\<@\>\""}], "]"}]}]}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mask", "=", 
   RowBox[{"Binarize", "[", 
    RowBox[{
    "Import", "[", 
     "\"\<http://pngimg.com/uploads/twitter/twitter_PNG35.png\>\"", "]"}], 
    "]"}]}], ";"}]}], "Input",
