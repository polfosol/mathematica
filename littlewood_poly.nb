Cell["A fancier function with a slight increase in speed:", "Subsubsection",ExpressionUUID->"a93a6bc5-6f80-4dec-ae4a-9a87547c9678"],

Cell[BoxData[
 RowBox[{
  RowBox[{"fastRootCalculate", "[", 
   RowBox[{"n_Integer", ",", 
    RowBox[{"x_", ":", 
     RowBox[{"Unique", "[", "]"}]}]}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"poly", "=", 
       RowBox[{
        RowBox[{"1", "+", "#", "+", 
         RowBox[{
          RowBox[{"Tuples", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"1", ",", 
              RowBox[{"-", "1"}]}], "}"}], ",", 
            RowBox[{"n", "-", "1"}]}], "]"}], ".", " ", 
          RowBox[{"#", "^", 
           RowBox[{"Range", "[", 
            RowBox[{"2", ",", "n"}], "]"}]}]}]}], "&"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"solve", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"$VersionNumber", "<", "12.3"}], ",", 
         RowBox[{
          RowBox[{"x", "/.", 
           RowBox[{"NSolve", "@", "##"}]}], "&"}], ",", "NSolveValues"}], 
        "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"mirror", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"n", ">", "3"}], ",", "Thread", ",", "Identity"}], "]"}], 
         "@", 
         RowBox[{"Join", "[", 
          RowBox[{"#", ",", 
           RowBox[{"-", "#"}]}], "]"}]}], "&"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"map", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"n", ">", "5"}], ",", "ParallelMap", ",", "Map"}], "]"}]}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{"k", "=", 
       RowBox[{
        RowBox[{"2", "$ProcessorCount"}], "-", "$KernelCount"}]}]}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"k", ">", "0"}], "&&", 
        RowBox[{"n", ">", "5"}]}], ",", 
       RowBox[{"LaunchKernels", "@", "k"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"solve", "[", 
          RowBox[{
           RowBox[{"#", "==", "0"}], ",", "x"}], "]"}], "&"}], ",", 
        RowBox[{"poly", "@", "x"}]}], "]"}], "//", "mirror"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.847034434918005*^9, 3.8470345327962523`*^9}, {
