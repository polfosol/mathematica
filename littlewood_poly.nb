Cell["A fancier function with a slight increase in speed:", "Subsubsection",ExpressionUUID->"d148e367-3c33-4a09-ad26-f5d5f60d37bd"],

Cell[BoxData[
 RowBox[{
  RowBox[{"polynomialRoots", "[", 
   RowBox[{"n_Integer", ",", 
    RowBox[{"x_", ":", 
     RowBox[{"Unique", "[", "]"}]}], ",", 
    RowBox[{"graph_", ":", "0"}]}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"map", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"n", ">", "5"}], ",", "ParallelMap", ",", "Map"}], "]"}]}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{"solve", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"$VersionNumber", "<", "12.3"}], ",", 
         RowBox[{
          RowBox[{"x", "/.", " ", 
           RowBox[{"NSolve", "@", "##"}]}], "&"}], ",", "NSolveValues"}], 
        "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"break", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"TrueQ", "@", "graph"}], ",", "ReIm", ",", "Identity"}], 
        "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"mirror", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"n", ">", "3"}], ",", "Thread", ",", "Identity"}], "]"}], 
         "@", 
         RowBox[{"Join", "[", 
          RowBox[{"#", ",", 
           RowBox[{"-", "#"}]}], "]"}]}], "&"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"poly", "=", 
       RowBox[{
        RowBox[{"1", "+", "#", "+", 
         RowBox[{
          RowBox[{"Tuples", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"1", ",", 
              RowBox[{"-", "1"}]}], "}"}], ",", 
            RowBox[{"n", "-", "1"}]}], "]"}], ".", " ", 
          RowBox[{"#", "^", 
           RowBox[{"Range", "[", 
            RowBox[{"2", ",", "n"}], "]"}]}]}]}], "&"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"map", "===", "ParallelMap"}], " ", "&&", " ", 
        RowBox[{"$KernelCount", "<", 
         RowBox[{"2", "$ProcessorCount"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"LaunchKernels", "[", 
        RowBox[{
         RowBox[{"2", "$ProcessorCount"}], "-", "$KernelCount"}], "]"}]}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"break", "@", 
          RowBox[{"solve", "[", 
           RowBox[{
            RowBox[{"#", "==", "0"}], ",", "x"}], "]"}]}], "&"}], ",", 
        RowBox[{"poly", "@", "x"}]}], "]"}], "//", "mirror"}]}]}], 
   "]"}]}]], "Input",
