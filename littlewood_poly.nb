Cell["A fancier function with a slight increase in speed:", "Subsubsection",ExpressionUUID->"5278d820-c3b0-42c9-b4bc-1727813bd69e"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"showRoots", "[", "n_", "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"s", "=", 
        RowBox[{"Which", "[", 
         RowBox[{
          RowBox[{"n", "<", "8"}], ",", "Medium", ",", 
          RowBox[{"n", "<", "14"}], ",", "Small", ",", "True", ",", "Tiny"}], 
         "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"r", "=", 
        RowBox[{"Thread", "@", 
         RowBox[{"ParallelMap", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"NSolveValues", "[", 
             RowBox[{
              RowBox[{"#", "==", "0"}], ",", "x"}], "]"}], "&"}], ",", 
           RowBox[{"littlewood", "[", "n", "]"}]}], "]"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"o", "=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"n", ">", "12"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"ImageSize", "\[Rule]", "1500"}], ",", 
            RowBox[{"Axes", "->", "False"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"ImageSize", "\[Rule]", "Large"}], "}"}]}], "]"}]}]}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"ComplexListPlot", "@@", 
      RowBox[{"{", 
       RowBox[{"r", ",", 
        RowBox[{"Sequence", "@@", "o"}], ",", 
        RowBox[{"PlotStyle", "\[Rule]", 
         RowBox[{"PointSize", "@", "s"}]}]}], "}"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"use", " ", 
    RowBox[{"List", "@@", 
     RowBox[{"Last", "/@", "NRoots"}]}], " ", "if", " ", "NSolveValues", " ", 
    "is", " ", "not", " ", "supported"}], " ", "*)"}]}]}], "Input",
